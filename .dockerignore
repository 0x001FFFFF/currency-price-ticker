# Ignore files that shouldn't be copied to Docker image
.git
.gitignore
.editorconfig
.env*
docker-compose*.yml
Dockerfile*
README.md
.memory_bank/
notes/
.vscode/
.idea/

# Development files
phpunit.dist.xml
phpstan.neon
.php-cs-fixer.dist.php

# Runtime directories (will be created in container)
var/cache/*
var/log/*
!var/cache/.gitkeep
!var/log/.gitkeep

# Dependencies (will be installed in container)
vendor/
node_modules/

# OS files
.DS_Store
Thumbs.db